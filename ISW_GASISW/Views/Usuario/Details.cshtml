@model ISW_GASISW.Models.usuario

@{
    ViewBag.Title = "Detalle - Usuario";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <div class="panel-title">
            Detalles - Usuario
        </div>
    </div>
    <div class="panel-body">
        <fieldset>
            <div class="form-group">
                <div class="editor-label">
                    Nombre de Usuario
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Rol del Usuario
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.rol.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Empleado
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.empleado.nombre1)
                </div>
            </div>
        </fieldset>
        <ul class="nav nav-pills">
            <li role="presentation">@Html.ActionLink("Edit", "Edit", new { id = Model.id }) </li>
            <li role="presentation">@Html.ActionLink("Back to List", "Index")</li>
        </ul>
    </div>
</div>