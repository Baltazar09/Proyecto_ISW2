@model ISW_GASISW.Models.accesos

@{
    ViewBag.Title = "Detalle - Permisos Asignados a Rol";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <div class="panel-title">
            Detalles - Permisos Asignados a Rol
        </div>
    </div>
    <div class="panel-body">
        <fieldset>
            <div class="form-group">
                <div class="editor-label">
                    Rol
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.rol.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Area de Acceso
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.formularios.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Permisos de Acceso
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.permisos.nombre)
                </div>
            </div>
        </fieldset>
        <ul class="nav nav-pills">
            <li role="presentation">@Html.ActionLink("Edit", "Edit", new { id = Model.id }) </li>
            <li role="presentation">@Html.ActionLink("Back to List", "Index")</li>
        </ul>
    </div>
</div>