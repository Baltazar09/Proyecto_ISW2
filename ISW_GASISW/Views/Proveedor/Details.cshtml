@model ISW_GASISW.Models.proveedor

@{
    ViewBag.Title = "Detalles - Proveedor";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <div class="panel-title">
            Detalles - Proveedor
        </div>
    </div>
    <div class="panel-body">
        <fieldset>
            <div class="form-group">
                <div class="editor-label">
                    Nombre
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Telefono
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.telefono)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Email
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.email)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Direccion
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.direccion)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Municipio
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.municipio.nombre)
                </div>
            </div>
        </fieldset>
        <ul class="nav nav-pills">
            <li role="presentation">@Html.ActionLink("Edit", "Edit", new { id = Model.id }) </li>
            <li role="presentation">@Html.ActionLink("Back to List", "Index")</li>
        </ul>
    </div>
</div>