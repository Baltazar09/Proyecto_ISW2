@model ISW_GASISW.Models.caja

@{
    ViewBag.Title = "Detalles - Caja";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <div class="panel-title">
            Detalles - Caja
        </div>
    </div>
    <div class="panel-body">
        <fieldset>
            <div class="form-group">
                <div class="editor-label">
                    Nombre de Caja
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Numero de Serie
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.numero)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Sucursal
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.sucursal.nombre)
                </div>
            </div>
        </fieldset>
        <ul class="nav nav-pills">
            <li role="presentation">@Html.ActionLink("Edit", "Edit", new { id = Model.id }) </li>
            <li role="presentation">@Html.ActionLink("Back to List", "Index")</li>
        </ul>
    </div>
</div>