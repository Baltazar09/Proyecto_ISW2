@model ISW_GASISW.Models.municipio

@{
    ViewBag.Title = "Detalles - Municipio";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <div class="panel-title">
            Detalles - Municipio
        </div>
    </div>
    <div class="panel-body">
        <fieldset>
            <div class="form-group">
                <div class="editor-label">
                    Nombre del Municipio
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Departamento al que pertence
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.departamento.nombre)
                </div>
            </div>
        </fieldset>
        <ul class="nav nav-pills">
            <li role="presentation">@Html.ActionLink("Edit", "Edit", new { id = Model.id }) </li>
            <li role="presentation">@Html.ActionLink("Back to List", "Index")</li>
        </ul>
    </div>
</div>