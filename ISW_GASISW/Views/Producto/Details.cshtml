@model ISW_GASISW.Models.producto

@{
    ViewBag.Title = "Detalles - Producto";
}

<div class="panel panel-primary">
    <div class="panel-heading">
        <div class="panel-title">
            Detalles - Producto
        </div>
    </div>
    <div class="panel-body">
        <fieldset>
            <div class="form-group">
                <div class="editor-label">
                    Nombre del Producto
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Presentacion del Producto
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.presentacion_producto.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Categoria del Producto
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.categoria_producto.nombre)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Precio Unitario de venta
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.precio_venta_u)
                </div>
            </div>
            <div class="form-group">
                <div class="editor-label">
                    Estado del Producto
                </div>
                <div class="display-field">
                    @Html.DisplayFor(model => model.estado)
                </div>
            </div>
        </fieldset>
        <ul class="nav nav-pills">
            <li role="presentation">@Html.ActionLink("Edit", "Edit", new { id = Model.id }) </li>
            <li role="presentation">@Html.ActionLink("Back to List", "Index")</li>
        </ul>
    </div>
</div>